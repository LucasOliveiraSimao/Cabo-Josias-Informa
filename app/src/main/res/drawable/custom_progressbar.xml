<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape
            android:innerRadiusRatio="3"
            android:shape="ring"
            android:thicknessRatio="8"
            android:useLevel="false">
            <size
                android:width="48dp"
                android:height="48dp" />
            <gradient
                android:angle="0"
                android:endColor="@color/on_tertiary"
                android:startColor="@color/on_tertiary"
                android:type="sweep"
                android:useLevel="false" />
        </shape>
    </item>
</layer-list>
